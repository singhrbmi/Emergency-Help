<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="hacker.l.emergency_help.fragments.HomeFragment">

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="@dimen/margin10"
                app:cardCornerRadius="@dimen/margin5"
                app:cardElevation="@dimen/margin5">

                <LinearLayout

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">


                    <TextView
                        android:id="@+id/tv_address"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/margin14"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/black"
                        android:textSize="@dimen/margin14"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:id="@+id/layout_hospital"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/hospital" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Near Place"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/layout_social"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/help" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Emergency Help"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/layout_ambulance"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/call" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Govt No"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>
                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/ebolence" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text=" Call Ambulance"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/police" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Call Police"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/fire" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Call Fire"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/womenhelp" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Call Women Help"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lyout_barCode"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/sacnner" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="QR Code Scanner"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lyout_suraksha"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/suraksha" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Suraksha Kavach"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/lyout_account"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/account" />

                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Account"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/layout_police"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/station" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Suggestion/Complent "
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/layout_contacts"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/contacts" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Contacts"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>

                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:orientation="horizontal">


                            <LinearLayout
                                android:id="@+id/lyout_about"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/margin5"
                                    android:src="@drawable/about" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="About Us"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/layout_pilicesire"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">


                                <!--<ImageView-->
                                <!--android:layout_width="80dp"-->
                                <!--android:layout_height="60dp"-->
                                <!--android:layout_gravity="center"-->
                                <!--android:scaleType="fitCenter"-->
                                <!--android:src="@drawable/logo" />-->


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@drawable/znmgd"
                                    android:gravity="center"
                                    android:textSize="@dimen/textSize14" />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lyout_whistle"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight=".33"
                                android:background="@drawable/done_corner_round"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="60dp"
                                    android:layout_height="60dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/dimen_5_dp"
                                    android:src="@drawable/whistle" />


                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="@dimen/margin10"
                                    android:gravity="center"
                                    android:text="Whistle"
                                    android:textSize="@dimen/textSize14" />

                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>

</FrameLayout>
